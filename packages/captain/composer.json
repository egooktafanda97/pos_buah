{
  "name": "captain/laravel",
  "description": "captain setup programs",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "role": "Developer",
    "name": "ego oktafanda",
    "email": "egooktafanda1097@gmail.com"
  }],
  "autoload": {
    "psr-4": {
      "Captain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Captain\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "illuminate/support": "^9.0||^10.0||^11.0",
    "php-open-source-saver/jwt-auth": "^2.2",
    "spatie/laravel-permission": "^6.4",
    "symfony/yaml": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5||^10.0||^11.0",
    "orchestra/testbench": "^6.20||^7.0||^8.0",
    "illuminate/container": "^9.0||^10.0||^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Captain\\Providers\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
